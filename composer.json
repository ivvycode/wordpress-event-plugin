{
    "name": "ivvy/wp-event-plugin",
    "description": "A plugin that integrates iVvy event registrations with wordpress.",
    "type": "wordpress-plugin",
    "authors": [
        {
            "name": "iVvy",
            "email": "support@ivvy.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "composer/installers": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "IvvyEvent\\": "src/IvvyEvent"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "installer-name": "ivvy-event"
    }
}